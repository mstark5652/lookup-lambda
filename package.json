{
  "name": "lookup-lambda",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "standard": "^16.0.1"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "whois": "^2.13.3"
  },
  "scripts": {
    "prebuild": "npm prune --production",
    "build": "zip -r ./lambda-build.zip index.js node_modules/**/*",
    "predeploy": "npm run build",
    "deploy": "aws s3 cp lambda-build.zip s3://terraform-serverless-stage/v$(node -p \"require('./package.json').version\")/lambda-build.zip",
    "lint": "standard",
    "test": "export NODE_ENV=test && nyc mocha --recursive --colors"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
